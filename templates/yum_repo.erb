[mysql-cluster-<%= @temp[:version] -%>-community]
name=MySQL Cluster 8.0 Community
baseurl=http://repo.mysql.com/yum/mysql-cluster-<%= @temp[:version] -%>-community/<%= @temp[:os] -%>/<%= @temp[:os_ver] -%>/$basearch/
