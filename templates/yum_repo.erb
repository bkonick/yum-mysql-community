# Chef Managed repo file. Changes may be overwritten on next chef-client run.

[mysql<%= @temp[:version] -%>-community]
name=MySQL <%= @temp[:version] -%> Community Server
baseurl=http://repo.mysql.com/yum/mysql-<%= @temp[:version] -%>-community/<%= @temp[:os] -%>/<%= @temp[:os_ver] -%>/$basearch/
enabled=<%= @temp[:mysql_community_server] %>
gpgcheck=<%= @temp[:gpgcheck] %>
gpgkey=http://repo.mysql.com/RPM-GPG-KEY-mysql

[mysql-connectors-community]
name=MySQL Connectors Community
baseurl=http://repo.mysql.com/yum/mysql-connectors-community/<%= @temp[:os] -%>/<%= @temp[:os_ver] -%>/$basearch/
